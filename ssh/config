	HostName github.com
	IdentityFile ~/.ssh/id_ed25519
	CertificateFile ~/.ssh/id_ed255119.pub

Host *
	User scott
	IdentityFile ~/.ssh/id_ed25519
	CertificateFile ~/.ssh/id_ed255119.pub
